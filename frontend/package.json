{
  "name": "catherd-frontend",
  "version": "0.0.0",
  "scripts": {
    "compile": "gulp && tsc -p src/",
    "compile:watch": "tsc -p src/ -w",
    "serve": "lite-server",
    "prestart": "npm run compile",
    "start": "concurrently \"npm run compile:watch\" \"npm run serve\""
  },
  "dependencies": {
    "@angular/common": "^2.4.8",
    "@angular/compiler": "^2.4.8",
    "@angular/core": "^2.4.8",
    "@angular/http": "^2.4.8",
    "@angular/platform-browser": "^2.4.8",
    "@angular/platform-browser-dynamic": "^2.4.8",
    "@angular/router": "^3.4.8",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "rxjs": "^5.0.1",
    "sockjs-client": "^1.1.2",
    "systemjs": "^0.20.7",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@types/bootstrap": "^3.3.32",
    "@types/gulp": "^3.8.32",
    "@types/sockjs-client": "^1.0.30",
    "concurrently": "^3.3.0",
    "gulp": "^3.9.1",
    "lite-server": "^2.2.2",
    "typescript": "^2.1.6"
  }
}
