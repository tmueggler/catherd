{
  "name": "catherd-backend",
  "version": "0.0.0",
  "scripts": {
    "compile": "gulp && tsc -p src/",
    "prestart": "npm run compile",
    "start": "node src/app/server.js",
    "db:init": "npm run compile && node src/app/db.init.js",
    "db:clean": "npm run compile && node src/app/db.clean.js"
  },
  "dependencies": {
    "express": "^4.14.1",
    "rethinkdb": "^2.3.3",
    "sockjs": "^0.3.18"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/gulp": "^3.8.32",
    "@types/rethinkdb": "^2.3.2",
    "@types/sockjs-node": "^0.3.28",
    "gulp": "^3.9.1",
    "typescript": "^2.1.6"
  }
}
