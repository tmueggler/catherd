{
  "name": "catherd-backend",
  "version": "0.0.0",
  "scripts": {
    "compile": "gulp && tsc -p tsconfig.json",
    "build": "npm run compile",
    "prestart": "npm run compile",
    "start": "node test/node/server.js",
    "db:init": "npm run compile && node test/scripts/db.init.js",
    "db:clean": "npm run compile && node test/scripts/db.clean.js"
  },
  "dependencies": {
    "express": "^4.14.1",
    "mqtt": "^2.4.0",
    "rethinkdb": "^2.3.3",
    "sockjs": "^0.3.18",
    "stompjs": "^2.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/gulp": "^3.8.32",
    "@types/mqtt": "0.0.33",
    "@types/rethinkdb": "^2.3.2",
    "@types/sockjs-node": "^0.3.28",
    "@types/stompjs": "^2.3.0",
    "gulp": "^3.9.1",
    "typescript": "^2.1.6"
  }
}
